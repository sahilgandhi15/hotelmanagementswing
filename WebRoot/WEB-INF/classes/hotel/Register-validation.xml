<!DOCTYPE validators PUBLIC
        "-//OpenSymphony Group//XWork Validator 1.0.2//EN"
        "http://www.opensymphony.com/xwork/xwork-validator-1.0.2.dtd">

<validators>
    <field name="userName">
        <field-validator type="requiredstring">
            <message key="用户名不能为空"/>
        </field-validator>
    </field>
    <field name="loginName">
        <field-validator type="requiredstring">
            <message key="登录名不能为空"/>
        </field-validator>
    </field>
    <field name="password">
        <field-validator type="requiredstring">
            <message key="密码不能为空"/>
        </field-validator>
    </field>
    <field name="identifier">
        <field-validator type="requiredstring">
            <message key="身份证号不能为空"/>
        </field-validator>
    </field>
</validators>
